{
  "manifest_version": 3,
  "name": "Webpack WebExtension Reloader Sample",
  "version": "0.1",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "icons": {
    "16": "webpack.16.png",
    "48": "webpack.48.png",
    "128": "webpack.128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["<all_urls>"],
      "js": ["content-script.bundle.js"],
      "css": ["style.css"]
    }
  ]
}
